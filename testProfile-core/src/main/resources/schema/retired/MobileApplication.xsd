<?xml version="1.0" ?>
<xs:schema targetNamespace="http://schema.org/MobileApplication" xmlns:lt="http://courseload.com/nuxeo/listTypes" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:import namespace="http://courseload.com/nuxeo/listTypes" schemaLocation="listTypes.xsd"/>
	<xs:import namespace="http://schema.org/Thing" schemaLocation="Thing.xsd"/>
	<xs:import namespace="http://schema.org/CreativeWork" schemaLocation="CreativeWork.xsd"/>
	<xs:import namespace="http://schema.org/SoftwareApplication" schemaLocation="SoftwareApplication.xsd"/>
	<xs:element name="carrierRequirements" type="xs:string">
