<?xml version="1.0" ?>
<xs:schema targetNamespace="http://schema.org/Episode" xmlns:lt="http://courseload.com/nuxeo/listTypes" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:import namespace="http://courseload.com/nuxeo/listTypes" schemaLocation="listTypes.xsd"/>
	<xs:import namespace="http://schema.org/Thing" schemaLocation="Thing.xsd"/>
	<xs:import namespace="http://schema.org/CreativeWork" schemaLocation="CreativeWork.xsd"/>
	<xs:element name="actor" type="lt:textList">
		<xs:annotation>
			<xs:documentation>A cast member of the movie, tv/radio series, season, episode, or video.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="director" type="lt:textList">
		<xs:annotation>
			<xs:documentation>The director of the movie, tv/radio episode or series.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="episodeNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>Position of the episode within an ordered group of episodes.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="musicBy" type="xs:string">
		<xs:annotation>
			<xs:documentation>The composer of the movie or TV/radio soundtrack.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="partOfSeason" type="xs:string">
		<xs:annotation>
			<xs:documentation>The season to which this episode belongs.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="partOfSeries" type="xs:string">
		<xs:annotation>
			<xs:documentation>The series to which this episode or season belongs.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="position" type="xs:string">
		<xs:annotation>
			<xs:documentation>Free text to define other than pure numerical ranking of an episode or a season in an ordered list of items (further formatting restrictions may apply within particular user groups).</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="producer" type="xs:string">
		<xs:annotation>
			<xs:documentation>The producer of the movie, tv/radio series, season, or episode, or video.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="productionCompany" type="xs:string">
		<xs:annotation>
			<xs:documentation>The production company or studio that made the movie, tv/radio series, season, or episode, or media object.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="publication" type="xs:string">
		<xs:annotation>
			<xs:documentation>A publication event associated with the episode, clip or media object.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="trailer" type="xs:string">
		<xs:annotation>
			<xs:documentation>The trailer of a movie or tv/radio series, season, or episode.</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>
